<div class="table table-striped table-bordered table-responsive">
  <table class="table">
    <thead>
      <tr>
        <th>ToDo</th>
        <th>Days Left</th>
        <th>Complete</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <%= form_for List.new do |form| %>
        <td><%= form.text_field :todo, placeholder: 'New ToDo' %><%= form.submit 'Save' %> </td>
        <% end %>
        <td>7</td>
        <td><% check_box_tag(:YesorNo) %><%= label_tag(:YesorNo, "Yes/No") %></td>
      </tr>
    </tbody>
  </table>
</div>
